<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.gdse.pcstore.controller.LoginHistoryController">
   <children>
      <VBox layoutX="11.0" layoutY="12.0" prefHeight="697.0" prefWidth="978.0" spacing="20.0" style="-fx-background-color: #fff;">
         <children>
            <TableView fx:id="tblLoginHistory" onMouseClicked="#onClickTable" prefHeight="637.0" prefWidth="938.0" VBox.vgrow="ALWAYS">
               <columns>
                  <TableColumn fx:id="colLoginId" prefWidth="75.0" text="Login Id" />
                  <TableColumn fx:id="colUserId" prefWidth="75.0" text="User Id" />
                  <TableColumn fx:id="colName" prefWidth="75.0" text="User Name" />
                  <TableColumn fx:id="colDate" prefWidth="75.0" text="Date" />
                  <TableColumn fx:id="colTime" prefWidth="75.0" text="Time" />
                  <TableColumn fx:id="colRole" prefWidth="75.0" text="Role" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <VBox prefHeight="263.0" prefWidth="938.0" spacing="10.0">
               <children>
                  <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="160.0" prefHeight="27.2">
                     <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="27.2" HBox.hgrow="ALWAYS">
                           <children>
                              <Label prefHeight="17.0" prefWidth="130.0" text="Employee Id : " HBox.hgrow="ALWAYS">
                                 <font>
                                    <Font name="Calibri" size="14.0" />
                                 </font>
                              </Label>
                              <Label fx:id="lblEmployeeId" layoutX="10.0" layoutY="15.0" maxWidth="500.0" prefHeight="18.0" prefWidth="300.0" HBox.hgrow="ALWAYS">
                                 <font>
                                    <Font name="Calibri" size="14.0" />
                                 </font>
                              </Label>
                              <Label prefHeight="17.0" prefWidth="84.0" text="Role : " HBox.hgrow="ALWAYS">
                                 <font>
                                    <Font name="Calibri" size="14.0" />
                                 </font>
                              </Label>
                              <Label fx:id="lblRole" prefWidth="150.0">
                                 <font>
                                    <Font name="Calibri" size="14.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="27.0" spacing="15.0">
                     <children>
                        <Label prefHeight="17.0" prefWidth="64.0" text="Name : " HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="Calibri" size="14.0" />
                           </font>
                        </Label>
                        <Label fx:id="lblName" prefHeight="17.0" prefWidth="253.0">
                           <font>
                              <Font name="Calibri" size="14.0" />
                           </font>
                        </Label>
                        <Label prefHeight="17.0" prefWidth="51.0" text="NIC : " HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="Calibri" size="14.0" />
                           </font>
                        </Label>
                        <Label fx:id="lblNic" prefHeight="17.0" prefWidth="202.0">
                           <font>
                              <Font name="Calibri" size="14.0" />
                           </font>
                        </Label>
                        <Label prefHeight="17.0" prefWidth="70.0" text="Phone : ">
                           <font>
                              <Font name="Calibri" size="14.0" />
                           </font>
                        </Label>
                        <Label fx:id="lblPhone" prefHeight="17.0" prefWidth="190.0">
                           <font>
                              <Font name="Calibri" size="14.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" prefHeight="27.0" spacing="15.0">
                     <children>
                        <Label prefHeight="17.0" prefWidth="64.0" text="Email :" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="Calibri" size="14.0" />
                           </font>
                        </Label>
                        <Label fx:id="lblEmail" prefHeight="17.0" prefWidth="488.0">
                           <font>
                              <Font name="Calibri" size="14.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
